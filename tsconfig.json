{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "paths": {
      "@studio/types": [
        "./sanity.types.ts"
      ],
      "@studio/*": [
        "./studio/*"
      ],
      "@lib/*": [
        "./src/lib/*"
      ],
      "@/*": [
        "./src/*"
      ]
    },
  }
}
